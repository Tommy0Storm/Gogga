{
  "name": "gogga-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "rm -rf .next 2>/dev/null || true && ./scripts/update-lan-ip.sh",
    "dev": "next dev -H 192.168.0.130 --experimental-https --experimental-https-key ./certs/key.pem --experimental-https-cert ./certs/cert.pem",
    "dev:https": "next dev -H 192.168.0.130 --experimental-https --experimental-https-key ./certs/key.pem --experimental-https-cert ./certs/cert.pem",
    "dev:lan": "next dev -H 192.168.0.130 --experimental-https --experimental-https-key ./certs/key.pem --experimental-https-cert ./certs/cert.pem",
    "dev:localhost": "next dev --experimental-https --experimental-https-key ./certs/key.pem --experimental-https-cert ./certs/cert.pem",
    "dev:http": "next dev -H 192.168.0.130 -p 3000",
    "dev:docker": "next dev -H 192.168.0.130 -p 3000",
    "dev:network": "concurrently \"next dev -p 3000\" \"node scripts/network-proxy.js 3001 3000\"",
    "proxy": "node scripts/network-proxy.js",
    "clean": "rm -rf .next node_modules/.cache",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
