{
  "name": "gogga-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "rm -rf .next 2>/dev/null || true",
    "dev": "next dev -H 0.0.0.0 --experimental-https --experimental-https-key ./certs/key.pem --experimental-https-cert ./certs/cert.pem",
    "dev:http": "next dev -H 0.0.0.0 -p 3000",
    "dev:network": "concurrently \"next dev -p 3000\" \"node scripts/network-proxy.js 3001 3000\"",
    "proxy": "node scripts/network-proxy.js",
    "clean": "rm -rf .next node_modules/.cache",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "@emailjs/nodejs": "^5.0.2",
    "@huggingface/transformers": "^3.8.1",
    "@libsql/client": "^0.15.15",
    "@openrouter/sdk": "^0.2.9",
    "@prisma/adapter-better-sqlite3": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "better-sqlite3": "^12.5.0",
    "dotenv": "^16.5.0",
    "axios": "^1.6.2",
    "dexie": "^4.2.1",
    "flexsearch": "^0.8.212",
    "jszip": "^3.10.1",
    "next": "^16.0.7",
    "next-auth": "5.0.0-beta.30",
    "onnxruntime-web": "^1.23.2",
    "posthog-js": "^1.302.2",
    "posthog-node": "^5.17.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.5.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "unpdf": "^1.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20.10.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "ignore-loader": "^0.1.2",
    "lucide-react": "^0.555.0",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.32",
    "prisma": "^7.1.0",
    "@types/better-sqlite3": "^7.6.13",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.3.3"
